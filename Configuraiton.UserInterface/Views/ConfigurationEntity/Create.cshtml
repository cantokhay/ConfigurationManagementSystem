@model CreateConfigurationEntityVM

@{
    ViewData["Title"] = "Create";
}

<h1>Create Configuration Entity</h1>
<div>
    <form method="post">
        <div class="form-group">
            <label asp-for="Name"></label>
            <input type="text" asp-for="Name" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="ApplicationName"></label>
            <input type="text" asp-for="ApplicationName" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Type"></label>
            <input type="text" asp-for="Type" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Value"></label>
            <input type="text" asp-for="Value" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="IsActive"></label>
            <input type="checkbox" asp-for="IsActive" class="form-control" />
        </div>
        <button type="submit" class="btn btn-primary">Create</button>
    </form>
</div>
